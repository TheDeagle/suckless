#opening dmenu
super + p
	dmenu_run
#openin terminal
ctrl + alt + Return
	st
#locking screen
super + l
	/usr/loacl/bin/slock
#screenshot
Print
	$HOME/bin/scrot-full
super + Print
	$HOME/bin/dmenu-screenshot#opening terminal
#focus on the stack
alt + Left
	dwmc focusstack -
#focus down
alt + Right
	dwmc focusstack +
#toggle bar
F11
	dwmc togglebar
#kill client
ctrl + q
	dwmc killclient
#resize client
KP_Left
	dwmc setmfact -0.015
KP_Right
	dwmc setmfact 0.015+
#switch windows
ctrl + Return
	dwmc zoom

